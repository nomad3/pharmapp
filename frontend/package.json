{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@react-google-maps/api": "^2.20.8",
    "axios": "^1.7.9",
    "cra-template": "1.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.13.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.7.0",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "^react-router$": "react-router/dist/development/index.js",
      "^react-router/dom$": "react-router/dist/development/dom-export.js",
      "^react-router-dom$": "react-router-dom/dist/index.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios|react-router|react-router-dom)/)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2"
  }
}
